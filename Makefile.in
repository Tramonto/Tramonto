# # Use the configure script to convert this Makefile.in file to a Makefile #


@SET_MAKE@
srcdir = @srcdir@
VPATH = @srcdir@

all: dftTarget

dftTarget:
	(cd src; ${MAKE})
	cp src/dft@EXEEXT@ dft@EXEEXT@

clean:
	-(cd src; ${MAKE} clean)
	-(cd Documentation; ${MAKE} clean)
	-rm Makefile

doc:
	(cd doc; ${MAKE})

