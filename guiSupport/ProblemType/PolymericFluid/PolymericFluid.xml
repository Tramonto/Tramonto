<Maui>


<!--
    These files contain data common to
    a polymeric fluid problem regardless
    of dimensionality (ie. 1D, 2D or 3D)
-->
<Import filename="PolymericFluidType.xml" />
<Import filename="PolymericFunctionals.xml" />
<Import filename="BoundaryAndStateParametersPolymeric.xml"/>


<!--
    The base class for polymeric problems
-->
<Class type="PolymericFluid" label="Polymeric fluid" base="ProblemType">
    <Fields>
        <PolymericFluidType name="polymerFluidType" label="Polymer types"/>
    </Fields>
</Class>


<!--
    These files describe the specific 
    aspects of polymeric problems
    that depend on dimensionality
-->
<Import filename="1D/OneDPolymericProblem.xml"/>
<Import filename="2D/TwoDPolymericProblem.xml"/>
<Import filename="3D/ThreeDPolymericProblem.xml"/>

</Maui>


