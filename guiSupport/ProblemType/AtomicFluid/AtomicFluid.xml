<Maui>

    
    <!--
        These files contain data common to
        an atomic fluid problem regardless
        of dimensionality (ie. 1D, 2D or 3D)
    -->
    <Import filename="FluidFunctionals.xml" />

    
    
    <!--
        The base class for polymeric problems
    -->
    <Class type="AtomicFluid" label="Atomic fluid" base="ProblemType">
    </Class>
    
    
    <!--
        These files describe the specific 
        aspects of atomic problems
        that depend on dimensionality
    -->
    <Import filename="1D/OneDAtomicProblem.xml"/>
    <Import filename="2D/TwoDAtomicProblem.xml"/>
    <Import filename="3D/ThreeDAtomicProblem.xml"/>

</Maui>


