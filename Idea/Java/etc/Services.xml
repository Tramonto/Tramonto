  <Class label="User services" type="Services">
    <Action label="Submit" class="Maui.Interface.SubmitAction"/>
    <Action label="View compressed" class="Maui.Interface.ViewAction"/>
    <Action verbose="true" label="View verbose" class="Maui.Interface.ViewAction"/>
    <Action verbose="true" label="Save XML" dir="$HOME" class="Maui.Interface.SaveAction"/>
